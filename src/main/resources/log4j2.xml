<?xml version="1.0" encoding="utf-8"?>
<Configuration status="WARN">
    <Appenders>
            <Console name="Console" targets="SYSTEM_OUT">
                    <PatternLayout pattern="%{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
            </Console>
    </Appenders>
    <Loggers>
            <Root level="info">
                    <AppenderRef ref="Console"/>
            </Root>
    </Loggers>
</Configuration>